source ~/.bash_profile
source ~/prompt_256.sh
source ~/.zsh/zsh-autosuggestions/zsh-autosuggestions.zsh
source /home/chiahao/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh
# up
function up_widget() {
	BUFFER="cd .."
	zle accept-line
}
zle -N up_widget
bindkey "^k" up_widget
bindkey '^ ' autosuggest-accept

CASE_SENSITIVE="false"

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
